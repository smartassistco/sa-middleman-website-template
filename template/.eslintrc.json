{
  "root": true,
  "plugins": [
  ],
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "comma-dangle": 0,
    "max-len": 0,
    "object-curly-spacing": 0,
    "spaced-comment": 0,
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ]
  }
}
